{
  "name": "iehurtado/comments",
  "description": "Ejemplo de un paquete Laravel que incorpora Comentarios",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ignacio Hurtado",
      "email": "ignaciohurtado91@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Iehurtado\\Comments\\": "src",
      "Iehurtado\\Comments\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "Iehurtado\\Comments\\Tests\\": "tests"
      }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Iehurtado\\Comments\\CommentsServiceProvider"
          ]
      }
  },
  "require": {},
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  }
}
